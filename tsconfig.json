{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "noImplicitThis": true,
        "declaration": true,
        "removeComments": false,
        "noLib": false,
        "noEmit": true,
        "jsx": "react"
    },
    "include": [
        "packages/**/**.ts",
    ],
    "exclude": [
        "**/dist",
        "node_modules"
    ]
}